			"^uni-(.*)": ["@/components/uni-$1.vue", "@/uni_modules/uni-$1/components/uni-$1/uni-$1.vue"]
		}
	},
	"pages": [{
		"path": "pages/login/login"
	}, {
		"path": "pages/index/index"
	}, {
		"path": "pages/mine/mine"
	}, {
		"path": "pages/mine/changeInfo"
	}, {
		"path": "pages/mine/myComment"
	}, {
		"path": "pages/mine/replyMe"
